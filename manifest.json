{
  "manifest_version": 2,

  "name": "Killer Queen",
  "version": "0.1.0",
  "description": "...Maybe we're not supposed to know the description??",
  "permissions": [
    "tabs",
    "notifications",
    "http://*/",
    "https://*/"
  ],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "content_scripts": [{
    "css": ["styles.css"],
    "js": ["content.js", "popper.js", "tippy.js"],
    "matches": ["<all_urls>"]
  }]
}
